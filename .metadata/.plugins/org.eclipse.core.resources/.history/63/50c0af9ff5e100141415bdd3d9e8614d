import java.util.regex.Pattern;

public class Lab2 {
	String word1 = "there";
	String word2 = "which";
	String word3 = "their";
	String word4 = "about";
	String word5 = "these";
	String word6 = "words";
	String word7 = "would";
	String word8 = "other";
	String word9 = "write";
	String word10 = "could";

	String[] word;

	public static void main(String arg[]) {
		new Lab2().run();

	}

	public void run() {
		word = new String[] { word1, word2, word3, word4, word5, word6, word7,
				word8, word9, word10 };

		for (int i = 0; i < word.length; i++) {
			for (int k = 0; k < word.length; k++) {
				boolean match = word[i].contains(word[k].charAt(1)) && word[i].contains(word[k].charAt(1))
						&& word[i].contains(word[k].charAt(1)) && word[i].contains(word[k].charAt(1))
						&& word[i].(String)contains(word[k].charAt(1));
			}
		}

	}
}